<%- include("partials/header"); -%>

    <h1>
        <%=postTitle%>
    </h1>

    <div class="output">
        <%=postContent%>
    </div>
    <%if(postTitle==='Home' ){%>

        <% posts.forEach(function(post){ %>
            <h1>
                <%= post.title %>
            </h1>
            <div>
                <%= post.content.slice(0,100) %> ...
                    <a href="posts/<%=_.kebabCase(post.title) %>">Read More</a>
            </div>
            <% }) %>
                <%}%>



                    <%- include("partials/footer"); -%>